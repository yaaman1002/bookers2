<%= render 'books/list', user: @user, book: @book %>
         <div class="col-md-8 offset-md-1">
           <h2>Users</h2>
           <table class="table">
             <thead>
               <tr>
                 <th>image</th>
                 <th>name</th>
                 <th colspan="3"></th>
               </tr>
             </thead>
             <tbody>
              <% @users.each do |user| %>
               <tr>
              <td><p><%= image_tag user.get_profile_image(100,100) %></p></td>
              <td><p><%= user.name %></p></td>
              <td><p><%= link_to "Show", user_path(user.id) %></p></td>
              <% end %>
               </tr>
             </tbody>
           </table>
         </div>
        </div>
       </div>